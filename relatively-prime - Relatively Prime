#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

void solve(){
    ll n; cin>>n;
    ll count=0;
    for(ll i=1; i<=n; ++i) if(__gcd(i,n)==1) ++count;
    cout<<count <<'\n';
}


int main(){
    optimize();

    ll t=1;
    // cin>>t;
    while(t--)
        solve();
}
