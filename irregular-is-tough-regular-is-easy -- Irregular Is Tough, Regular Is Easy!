#include<bits/stdc++.h>
using namespace std;

#define optimize() ios_base::sync_with_stdio(0);cin.tie(0);cout.tie(0);

#define ll long long

//====================================

ll c = 1;

void solve(){
    ll n; cin>>n;
    ll ans = (n*(n-1)*(n-2)*(n-3))/24;
    cout<<"Case " <<c++ <<": ";
    cout<<ans <<'\n';
}


int main(){
    optimize();

    ll t=1;
    cin>>t;
    while(t--)
        solve();
}
